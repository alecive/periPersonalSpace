    </connection>
    <connection>
        <from>/icub/skin/torso_comp</from>
        <to>/skinGui/torso:i</to>
        <protocol>udp</protocol>
    </connection>
   
    <connection>
        <from>/icub/camcalib/left/out</from>
        <to>/leftCam</to>
        <protocol>udp</protocol>
    </connection>
    <connection>
        <from>/icub/camcalib/right/out</from>
        <to>/rightCam</to>
        <protocol>udp</protocol>
    </connection>
    
    
</application>
