<application>
    <name>doubleTouch</name>

    <dependencies>
    </dependencies>

    <module>
        <name>doubleTouch</name>
        <parameters></parameters>
        <node>icub15</node>
        <stdio></stdio>
        <tag>doubleTouch</tag>
    </module>
    <module>
        <name>pf3dTracker</name>
        <parameters>--from pf3dTracker.ini</parameters>
        <node>icub-b1</node>
        <tag>PF3DTracker</tag>
    </module>
    <module>
        <name>visuoTactileRF</name>
        <parameters></parameters>
        <node>icub15</node>
        <stdio></stdio>
        <tag>visuoTactileRF</tag>
    </module>
    <module>
        <name>visuoTactileWrapper</name>
        <parameters></parameters>
        <node>icub15</node>
        <stdio></stdio>
        <tag>visuoTactileWrapper</tag>
    </module>
    <module>
        <name>demoAvoidance</name>
        <parameters></parameters>
        <node>icub15</node>
        <stdio></stdio>
        <tag>demoAvoidance</tag>
    </module>
    <module>
        <name>motionCUT</name>
        <parameters>--recogThres 0.2</parameters>
        <node>icub-b11</node>
        <stdio></stdio>
        <tag>motionCUT</tag>
    </module>
    <module>
        <name>yarpview</name>
        <parameters>--name /vtRF/left --RefreshTime 33 --x 0 --y 0 --w 400 --h 400</parameters>
        <node>icub15</node>
        <stdio></stdio>
        <tag>yarpview</tag>
    </module>
    <module>
        <name>yarpview</name>
        <parameters>--name /vtRF/right --RefreshTime 33 --x 420 --y 0 --w 400 --h 400</parameters>
        <node>icub15</node>
        <stdio></stdio>
        <tag>yarpview</tag>
    </module>
    <module>
        <name>yarpview</name>
        <parameters>--name /PF3DTracker_viewer --RefreshTime 33 --x 840 --y 0 --w 400 --h 400</parameters>
        <node>icub15</node>
        <tag>PF3DTracker_viewer</tag>
    </module>
    <module>
        <name>iCubSkinGui</name>
        <parameters> --context periPersonalSpace --from left_forearm_vtRF.ini --name /vtRFSkinGui --useCalibration --xpos 0 --ypos 640 --width 400 --height 400</parameters>
        <node>icub16</node>
        <tag>iCubSkinGui_left_forearm</tag>
    </module>
    <module>
        <name>iCubSkinGui</name>
        <parameters> --context periPersonalSpace --from left_hand_vtRF.ini --name /vtRFSkinGui --useCalibration --xpos 420 --ypos 640 --width 400 --height 400</parameters>
        <node>icub16</node>
        <tag>iCubSkinGui_left_hand</tag>
    </module>
    <module>
        <name>iCubSkinGui</name>
        <parameters> --context periPersonalSpace --from right_forearm_vtRF.ini --name /vtRFSkinGui --useCalibration --xpos 840 --ypos 640 --width 400 --height 400</parameters>
        <node>icub16</node>
        <tag>iCubSkinGui_right_forearm</tag>
    </module>
    <module>
        <name>iCubSkinGui</name>
        <parameters> --context periPersonalSpace --from right_hand_vtRF.ini --name /vtRFSkinGui --useCalibration --xpos 1260 --ypos 640 --width 400 --height 400</parameters>
        <node>icub16</node>
        <tag>iCubSkinGui_right_hand</tag>
    </module>

    <connection>
        <from>/skinManager/skin_events:o</from>
        <to>/doubleTouch/contacts:i</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
        <from>/icub/camcalib/left/out</from>
        <to>/visuoTactileRF/imageL:i</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
        <from>/icub/camcalib/right/out</from>
        <to>/visuoTactileRF/imageR:i</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
        <from>/visuoTactileRF/imageL:o</from>
        <to>/vtRF/left</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
        <from>/visuoTactileRF/imageR:o</from>
        <to>/vtRF/right</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
        <from>/doubleTouch/status:o</from>
        <to>/visuoTactileRF/input:i</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
        <from>/doubleTouch/status:o</from>
        <to>/vtWThread/input:i</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
        <from>/icub/camcalib/left/out</from>
        <to>/icub/PF3DTracker_L/videoIn</to>
        <protocol>udp</protocol>
    </connection>
    <connection>
        <from>/icub/PF3DTracker_L/videoOut</from>
        <to>/PF3DTracker_viewer</to>
        <protocol>udp</protocol>
    </connection>
    <connection>
        <from>/icub/PF3DTracker_L/dataOut</from>
        <to>/visuoTactileWrapper/pf3dTracker:i</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
        <from>/icub/camcalib/left/out</from>
        <to>/motionCUT/img:i</to>
        <protocol>udp</protocol>
    </connection>
    <connection>
        <from>/motionCUT/img:o</from>
        <to>/PF3DTracker_viewer</to>
        <protocol>udp</protocol>
    </connection>
    <connection>
        <from>/motionCUT/blobs:o</from>
        <to>/visuoTactileWrapper/motionCUT:i</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
        <from>/visuoTactileWrapper/events:o</from>
        <to>/visuoTactileRF/events:i</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
	<from>/visuoTactileRF/skinGuiForearmL:o</from>
        <to>/vtRFSkinGui/left_forearm:i</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
	<from>/visuoTactileRF/skinGuiForearmR:o</from>
        <to>/vtRFSkinGui/right_forearm:i</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
	<from>/visuoTactileRF/skinGuiHandL:o</from>
        <to>/vtRFSkinGui/left_hand:i</to>
        <protocol>tcp</protocol>
    </connection>
    <connection>
	<from>/visuoTactileRF/skinGuiHandR:o</from>
        <to>/vtRFSkinGui/right_hand:i</to>
        <protocol>tcp</protocol>
    </connection>
</application>
